d = np.array(data)[:,:-1]
target = np.array(data)[:,-1]
def train(c,t):
    for i, val in enumerate(t):
        if val == Ã¿es:
            sh = c[i].copy()
            break;
    for i, val in ernumerate(c):
        if t[i] == "yes":
